{% extends 'base.html.twig' %}
{% block content %}
<div class="main-content">

{{ form_start(form) }}
    <div class="errors">
        {{ form_errors(form) }}
    </div>

    <div class="row">
        <div class="col" id="title">
            {{ form_row(form.Title) }}
        </div>
        <div class="col" id="description">
            {{ form_row(form.Description) }}
        </div>
        <div class="col" id="created_at">
            {{ form_row(form.created_at) }}
        </div>
        <div class="col" id="finished_at">
            {{ form_row(form.finished_at) }}
        </div>

    </div>
    <input type="submit" />
{{ form_end(form) }}
{% endblock %}
